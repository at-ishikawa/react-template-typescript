{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/flowtype",
        "prettier/react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-vars": 1
    },
    "settings": {
        "import/resolver": "webpack"
    }
}
